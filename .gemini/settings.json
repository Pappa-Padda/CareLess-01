{
  "mcpServers": {
    "mui-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@mui/mcp@latest"
      ]
    }
  },
  "context": {
    "loadMemoryFromIncludeDirectories": false
  },
  "tools": {
    "shell": {
      "showColor": true
    }
  },
  "ide": {
    "enabled": true
  },
  "experimental": {
    "introspectionAgentSettings": {
      "enabled": true
    }
  },
  "general": {
    "enablePromptCompletion": false
  }
}