{
  "mcpServers": {
    "mui-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@mui/mcp@latest"
      ]
    },
    "context7": {
      "httpUrl": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "ctx7sk-9ebe1558-24ef-46b7-ad9a-d0e23a2c6d04",
        "Accept": "application/json, text/event-stream"
      }
    }
  },
  "context": {
    "loadMemoryFromIncludeDirectories": false
  },
  "tools": {
    "shell": {
      "showColor": true
    }
  },
  "ide": {
    "enabled": true
  },
  "experimental": {
    "introspectionAgentSettings": {
      "enabled": true
    }
  },
  "general": {
    "enablePromptCompletion": false
  },
  "ui": {
    "showMemoryUsage": true
  }
}